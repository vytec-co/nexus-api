{
    "name": "{{ item.name }}",
    "online": true,
    "storage": {
      "blobStoreName": "default",
      "strictContentTypeValidation": true,
      "writePolicy": "allow_once"
    },
    "cleanup": {
      "policyNames": [
        "{{ item.policy|default('string') }}"
      ]
    },
    "component": {
      "proprietaryComponents": true
    },
    "apt": {
      "distribution": "bionic"
    },
    "aptSigning": {
      "keypair": "string",
      "passphrase": "string"
    }
  }